package com.Capg;

import java.util.Scanner;

public class Arrays {
	public static void main(String[] args) {
		int[] arr = new int[5];
		// Initializing an array Element
		arr[0] = 50;
		arr[1] = 100;
		arr[2] = 200;
		arr[3] = 300;
		arr[4] = 400;

		// Printing Array Elements:-
		for (int a : arr) {
			System.out.println(a);

		}

	}

}
